{
  "name": "Disable Content Security Policy",
  "description": "A extension that set csp value empty",
  "version": "1.0.0",
  "author": "Mywait",
  "homepage_url": "https://github.com/WithoutHair/Disable-Content-Security-Policy",
  "manifest_version": 2,
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "browsingData",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": ["background-v2.js"],
    "persistent": true
  },
  "browser_action": {}
}
