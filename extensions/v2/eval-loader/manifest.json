{
  "manifest_version": 2,
  "name": "Eval Loader PoC",
  "description": "Fetch remote script & eval() it",
  "version": "1.0",
  "permissions": [
    "https://localhost:8000/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
