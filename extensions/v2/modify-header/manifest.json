{
  "manifest_version": 2,
  "name": "Super Sound Booster",
  "version": "2.1.4",
  "description": "Enhance audio experience on all websites",
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": ["background-v2.js"],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Sound Booster"
  }
}