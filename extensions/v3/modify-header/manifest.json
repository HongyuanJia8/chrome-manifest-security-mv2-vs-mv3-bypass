{
  "manifest_version": 3,
  "name": "Super Sound Booster",
  "version": "2.1.4",
  "description": "Enhance audio experience on all websites",
  "permissions": [
    "tabs",
    "scripting",
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess",
    "storage"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Sound Booster"
  }
}